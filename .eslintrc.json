{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "max-len": 0,
        "no-undef": 0,
        "no-const-assign": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "import/no-unresolved": 0,
        "consistent-return":0,
        "new-cap":0
    }
}
